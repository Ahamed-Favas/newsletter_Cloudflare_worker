{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "my-newsletter",
  "main": "src/index.js",
  "compatibility_date": "2025-01-09",
  "observability": {
    "enabled": true
  },
  "triggers": {
    "crons": [
      "30 0,6,12,18 * * *",
      "30 18 */2 * *",
      "30 1 * * *"
    ]
  },
  "ai": {
    "binding": "AI"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "rss-feeds-d1",
      "database_id": "9201fab0-c3ad-46c1-a767-0b03451161c0"
    }
  ]
